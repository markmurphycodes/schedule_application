      </Label>
      <Label fx:id="loginLabel" layoutX="177.0" layoutY="96.0" text="Login">
         <font>
            <Font size="21.0" />
         </font>
      </Label>
      <Label fx:id="usernameLabel" layoutX="28.0" layoutY="149.0" text="Username:" />
      <Label fx:id="passwordLabel" layoutX="30.0" layoutY="201.0" text="Password:" />
